<b:isolate/>
<b:style src="./client.css"/>

<div class="item item_{selected}" event-click="select">
  <span class="num" b:show="{pickMode}">{num}</span>
  <span class="title title_{online}">{title}&nbsp;</span>
  <span b:show="{isBrowser}" class="location">{location}&nbsp;</span>
  <span b:show="{isNode}" class="pid">PID: {pid}&nbsp;</span>
</div>
